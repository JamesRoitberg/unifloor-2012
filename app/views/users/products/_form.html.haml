= form_for @users_product do |f|
  -if @users_product.errors.any?
    #error_explanation
      %h2= "#{pluralize(@users_product.errors.count, "error")} prohibited this users_product from being saved:"
      %ul
        - @users_product.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :name
    = f.text_field :name
  .field
    = f.label :category
    = f.text_field :category
  .actions
    = f.submit 'Save'
